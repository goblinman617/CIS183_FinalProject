<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_v_logIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/square_button"
        android:backgroundTint="#000000"
        android:text="Log In"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="237dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DM LogIn Page"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="42dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Other Buttons: Register -> Register Page"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="399dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="you dont need to enter info for now, just click log in"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="356dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Username:"
        android:textColor="#000000"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="116dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Password:"
        android:textColor="#000000"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="169dp" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="236dp"
        android:layout_height="45dp"
        android:ems="10"
        android:inputType="text"
        android:textColor="#000000"
        android:textColorHighlight="#5E5E5E"
        android:textColorLink="#757575"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="106dp" />

    <EditText
        android:id="@+id/editTextText2"
        android:layout_width="236dp"
        android:layout_height="45dp"
        android:ems="10"
        android:inputType="text"
        android:password="true"
        android:textColor="#000000"
        android:textColorHighlight="#5E5E5E"
        android:textColorLink="#757575"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="158dp" />

</androidx.constraintlayout.widget.ConstraintLayout>